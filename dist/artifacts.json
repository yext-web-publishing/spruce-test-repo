{
  "artifactStructure": {
    "assets": [
      {
        "root": "dist",
        "pattern": "assets/**/*"
      },
      {
        "root": "dist/public_assets",
        "pattern": "**/*"
      }
    ],
    "plugins": [
      {
        "pluginName": "PagesGenerator",
        "sourceFiles": [
          {
            "root": "dist/plugin",
            "pattern": "*{.ts,.json}"
          },
          {
            "root": "dist",
            "pattern": "assets/{server,static,renderer,render,client}/**/*{.js,.css}"
          }
        ],
        "event": "ON_PAGE_GENERATE",
        "functionName": "PagesGenerator"
      },
      {
        "pluginName": "metrics-43637",
        "event": "API",
        "functionName": "default",
        "apiPath": "{{locale}}/{{entityId}}/metrics",
        "sourceFiles": [
          {
            "root": "dist/functions/metrics-43637",
            "pattern": "*{.js,.ts}"
          }
        ]
      }
    ]
  }
}